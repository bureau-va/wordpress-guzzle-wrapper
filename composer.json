{
  "name": "maciekpaprocki/wordpress-guzzle-wrapper",
  "type": "library",
  "description": "Wordpress guzzle wrapper ",
  "keywords": [
    "maciekpaprocki",
    "wordpress-guzzle-wrapper"
  ],
  "homepage": "https://github.com/bureau-va/wordpress-guzzle-wrapper",
  "license": "MIT",
  "authors": [
    {
      "name": "Maciej Paprocki",
      "email": "maciekpaprocki@gmail.com",
      "homepage": "http://paprocki.co.uk",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "~5.5|~7.0",
    "pimple/pimple": "^3.0",
    "guzzlehttp/guzzle": "^6.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "scrutinizer/ocular": "~1.1",
    "squizlabs/php_codesniffer": "~2.3"
  },
  "autoload": {
    "psr-4": {
      "BureauVa\\WordpressGuzzle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BureauVa\\WordpressGuzzle\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
